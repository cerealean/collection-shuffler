{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 20",
    "_version": "20.1.0",
    "include": ["./src/**/*.ts"],
    "exclude": ["./src/**/*.test.ts"],
    "compilerOptions": {
      "lib": ["es2023"],
      "module": "node16",
      "target": "es2022",
  
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node16",
      "declaration": true,

      "outDir": "./lib"
    }
  }