{
    "$schema": "https://json.schemastore.org/swcrc",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "dynamicImport": false,
            "decorators": false
        },
        "target": "es2022",
        "loose": false,
        "externalHelpers": false,
        // Requires v1.2.50 or upper and requires target to be es2016 or upper.
        "keepClassNames": true,
        "baseUrl": "./",
        "minify": { 
            "compress": true,
            "mangle": true
         }
    },
    "isModule": true,
    "sourceMaps": true
}